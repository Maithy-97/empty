version: 1

# Files to open by default
tabs:
  - index.html
  - style.css
  - README.md

# Layout with browser preview
playground-view: terminal-editor-browser

# Start command (static preview-friendly)
start:
  command: echo "HTML/CSS lab ready. Open Browser Preview."

# Explicitly tell Fermion which file to preview
browser:
  entry: index.html
